{
  "name": "nuxtVuetify",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest",
    "test:e2e": "wdio wdio.conf.js",
    "test:e2e:tasks": "wdio wdio.conf.js --suite tasks",
    "test:e2e:comments": "wdio wdio.conf.js --spec ./test/e2e/specs/comments.spec.js",
    "test:e2e:login": "wdio wdio.conf.js --spec ./test/e2e/specs/login.spec.js"
  },
  "dependencies": {
    "@nuxt/content": "^1.9.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.0.2",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.1.0",
    "@wdio/cli": "^6.9.1",
    "@wdio/local-runner": "^6.9.1",
    "@wdio/mocha-framework": "^6.8.0",
    "@wdio/spec-reporter": "^6.8.1",
    "@wdio/sync": "^6.9.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "vue-jest": "^3.0.4"
  }
}
